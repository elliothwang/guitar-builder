<%- include('../partials/header') %>

  <h1>User's guitars</h1>

  <% userGuitars.forEach(function (userGuitar) { %>
    <tr>
      <td><%= userGuitar.name %></td>
      <td><%= userGuitar.body %></td>
      <td><%= userGuitar.neck %></td>
      <td><%= userGuitar.userName %></td>
      <td><%= userGuitar.userAvatar %></td>
    </tr>
  <% }); %>

<%- include('../partials/footer') %> 